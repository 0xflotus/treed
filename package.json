{
  "name": "treed",
  "version": "0.2.0",
  "description": "The main component of an editor for hierarchical content. ",
  "main": "index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jaredly/listless.git"
  },
  "keywords": [
    "list",
    "todo",
    "editor",
    "d3",
    "tree"
  ],
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "Apache v2",
  "bugs": {
    "url": "https://github.com/jaredly/listless/issues"
  },
  "dependencies": {
    "bluebird": "~1.2.4",
    "level-js": "~2.1.6",
    "react": "^0.11.1",
    "reactify": "^0.14.0",
    "superagent-browserify": "0.0.2",
    "jstransform": "~6.1.0",
    "envify": "~3.0.0",
    "marked": "~0.3.2",
    "localforage": "~0.9.2",
    "sublevel": "~2.4.0",
    "levelup": "~0.19.0",
    "async": "~0.9.0"
  },
  "devDependencies": {
    "reactify": "~0.13.1",
    "broccoli": "~0.12.3"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true,
          "everything": true,
          "visitors": [
            "jstransform/visitors/es6-destructuring-visitors"
          ]
        }
      ]
    ]
  },
  "atomify": {
    "js": {
      "entry": "index.js",
      "output": "demo/bundle.js",
      "transforms": [
        "reactify"
      ]
    },
    "css": {
      "entry": "index.less",
      "output": "demo/css/bundle.css"
    }
  }
}
